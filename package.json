{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning node, mongoDB, and express",
  "main": "app.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "import-dev": "node ./dev-data/data/import-dev-data.js --import",
    "delete-dev": "node ./dev-data/data/import-dev-data.js --delete",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "heroku-postbuild": "npm run build:js"
  },
  "author": "John Pittenger",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.22.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.16",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.30.4",
    "slugify": "^1.6.5",
    "stripe": "^9.1.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.6.2"
  }
}
